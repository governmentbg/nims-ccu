@model R_10019.PaperAttachedDocuments

@if (Model != null)
{
    <div class="table-wrapper">
        <img class="horizontal-scroll" src="@Url.Content(Links.Content.img.icons.blue_arrow_right_gif)" alt="" />
        <table class="chart-info dynamic-rows file-upload" width="100%">
            <thead>
                <tr>
                    <th style="width:500px;">@PaperAttachedDocument.Type</th>
                    <th>@PaperAttachedDocument.Description</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.PaperAttachedDocumentCollection != null && Model.PaperAttachedDocumentCollection.Count > 0)
                {
                    for (int i = 0; i < Model.PaperAttachedDocumentCollection.Count; i++)
                    {
                        @Html.DisplayFor(m => m.PaperAttachedDocumentCollection[i])
                    }
                }
            </tbody>
        </table>
    </div>
}
