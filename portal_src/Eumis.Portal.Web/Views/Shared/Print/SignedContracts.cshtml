@model R_10040.SignedContracts

@if (Model != null)
{
    <div>
        <table>
            <thead>
                <tr>
                    <th>@AttachedDocument.Description</th>
                    <th>@AttachedDocument.FileName</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.SignedContractCollection != null && Model.SignedContractCollection.Count > 0)
                {
                    for (int i = 0; i < Model.SignedContractCollection.Count; i++)
                    {
                        <tr>
                            <td>
                                @Model.SignedContractCollection[i].Description
                            </td>
                            <td>
                                @if (Model.SignedContractCollection[i].AttachedDocumentContent != null)
                                {
                                    <text>@Model.SignedContractCollection[i].AttachedDocumentContent.FileName</text>
                                }
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
}