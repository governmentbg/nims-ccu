                    {
                        <p>@Html.Partial(MVC.Shared.Views.Print.PublicNomenclature, item ?? new R_10001.PublicNomenclature())</p>
                    }
                }
            </td>
        </tr>

        <tr>
            <td>@InterventionCategoryDimensions.ThematicObjective</td>
            <td>
                @if (!isModelNull && Model.ThematicObjectiveCollection != null && Model.ThematicObjectiveCollection.Count > 0)
                {
                    foreach (var item in Model.ThematicObjectiveCollection)
