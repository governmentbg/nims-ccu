@model R_10005.InterventionCategoryDimensions

@{bool isModelNull = Model == null;}

<table class="intervention-category">
    <thead>
        <tr>
            <th>@InterventionCategoryDimensions.Dimension</th>
            <th>@InterventionCategoryDimensions.Value</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td style="width: 35%;">@InterventionCategoryDimensions.InterventionField</td>
            <td style="width: 65%;">
                @if (!isModelNull && Model.InterventionFieldCollection != null && Model.InterventionFieldCollection.Count > 0)
                {
                    foreach (var field in Model.InterventionFieldCollection)
                    {
                        <p>@Html.Partial(MVC.Shared.Views.Print.PublicNomenclature, field ?? new R_10001.PublicNomenclature())</p>
                    }
                }
            </td>
        </tr>

        <tr>
            <td>@InterventionCategoryDimensions.FormOfFinance</td>
            <td>
                @if (!isModelNull && Model.FormOfFinanceCollection != null && Model.FormOfFinanceCollection.Count > 0)
                {
                    foreach (var item in Model.FormOfFinanceCollection)
                    {
                        <p>@Html.Partial(MVC.Shared.Views.Print.PublicNomenclature, item ?? new R_10001.PublicNomenclature())</p>
                    }
                }
            </td>
        </tr>

        <tr>
            <td>@InterventionCategoryDimensions.TerritorialDimension</td>
            <td>
                @if (!isModelNull && Model.TerritorialDimensionCollection != null && Model.TerritorialDimensionCollection.Count > 0)
