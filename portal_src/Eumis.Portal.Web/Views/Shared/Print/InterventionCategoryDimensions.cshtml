                    }
                }
            </td>
        </tr>

        <tr>
            <td>@InterventionCategoryDimensions.EconomicDimension</td>
            <td>
                @if (!isModelNull && Model.EconomicDimensionCollection != null && Model.EconomicDimensionCollection.Count > 0)
                {
                    foreach (var item in Model.EconomicDimensionCollection)
                    {
                        <p>@Html.Partial(MVC.Shared.Views.Print.PublicNomenclature, item ?? new R_10001.PublicNomenclature())</p>
                    }
                }
            </td>
        </tr>
    </tbody>
</table>
