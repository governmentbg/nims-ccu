                    {
                        <p>@Html.Partial(MVC.Shared.Views.Print.PublicNomenclature, item ?? new R_10001.PublicNomenclature())</p>
                    }
                }
            </td>
        </tr>

        <tr>
            <td>@InterventionCategoryDimensions.TerritorialDeliveryMechanism</td>
            <td>
                @if (!isModelNull && Model.TerritorialDeliveryMechanismCollection != null && Model.TerritorialDeliveryMechanismCollection.Count > 0)
                {
