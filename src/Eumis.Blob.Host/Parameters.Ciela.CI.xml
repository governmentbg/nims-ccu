<?xml version="1.0" encoding="utf-8" ?>
<parameters>
    <parameter name="ErrorfileFilename">
        <parameterValidation kind="AllowEmpty" />
        <parameterEntry
            kind="XmlFile"
            scope="\\web.config$"
            match="/configuration/nlog/targets/target[@name='errorfile']/target/@fileName" />
    </parameter>
</parameters>
