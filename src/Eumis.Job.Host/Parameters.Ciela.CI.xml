<?xml version="1.0" encoding="utf-8" ?>
<parameters>
    <parameter name="Eumis.ApplicationServices:PortalLocationForAPIs">
        <parameterValidation kind="AllowEmpty" />
        <parameterEntry
            kind="XmlFile"
            scope="\\web.config$"
            match="/configuration/appSettings/add[@key='Eumis.ApplicationServices:PortalLocationForAPIs']/@value" />
    </parameter>
    <parameter name="Eumis.Job.Host:PortalUrl">
        <parameterValidation kind="AllowEmpty" />
        <parameterEntry
            kind="XmlFile"
            scope="\\web.config$"
            match="/configuration/appSettings/add[@key='Eumis.Job.Host:PortalUrl']/@value" />
    </parameter>
    <parameter name="Eumis.Job.Host:PortalReportUrl">
        <parameterValidation kind="AllowEmpty" />
        <parameterEntry
            kind="XmlFile"
            scope="\\web.config$"
            match="/configuration/appSettings/add[@key='Eumis.Job.Host:PortalReportUrl']/@value" />
    </parameter>
    <parameter name="Eumis.Job.Host:SystemUrl">
        <parameterValidation kind="AllowEmpty" />
        <parameterEntry
            kind="XmlFile"
            scope="\\web.config$"
            match="/configuration/appSettings/add[@key='Eumis.Job.Host:SystemUrl']/@value" />
    </parameter>
    <parameter name="ErrorfileFilename">
        <parameterValidation kind="AllowEmpty" />
        <parameterEntry
            kind="XmlFile"
            scope="\\web.config$"
            match="/configuration/nlog/targets/target[@name='errorfile']/target/@fileName" />
    </parameter>
</parameters>
